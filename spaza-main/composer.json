{
  "name": "spaza/spaza-main",
  "description": "This is my PHP project.",
  "require": {
    "php": "^8.1"
  },
  "license": "MIT",
  "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Controller\\": "controller/"
    }
  },
  "autoload": {
    "psr-0": {
      "App\\": "app/",
      "Controller\\": "controller/"
    }
  }
}